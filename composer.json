{
    "name": "ekmungai/eloquent-ifrs",
    "description": "Eloquent Double Entry Accounting with focus on IFRS Compliant Reporting",
    "keywords": [
        "accounting",
        "eloquent",
        "laravel",
        "lumen",
        "reporting"
    ],
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/ekmungai/eloquent-ifrs/issues",
        "source": "https://github.com/ekmungai/eloquent-ifrs"
    },
    "authors": [
        {
            "name": "Edward Mungai",
            "email": "emungai@karanjamungai.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.1",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/auth": "^10.0|^11.0|^12.0",
        "doctrine/dbal": "^2.10|^3.1|^4.2"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^1.6.4|^2.0",
        "laravel/legacy-factories": "1.x-dev|^1.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^9.4|^11.5.3",
        "squizlabs/php_codesniffer": "*",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "IFRS\\": "src"
        },
        "classmap": [
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "IFRS\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IFRS\\IFRSServiceProvider"
            ]
        }
    }
}

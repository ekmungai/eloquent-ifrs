{
    "name": "ekmungai/eloquent-ifrs",
    "description": "Eloquent Double Entry Accounting with focus on IFRS Compliant Reporting",
    "keywords": [
        "accounting",
        "eloquent",
        "laravel",
        "lumen",
        "reporting"
    ],
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/ekmungai/eloquent-ifrs/issues",
        "source": "https://github.com/ekmungai/eloquent-ifrs"
    },
    "authors": [
        {
            "name": "Edward Mungai",
            "email": "emungai@karanjamungai.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0.2",
        "illuminate/database": "^8.4|^9.0|^10.0",
        "illuminate/auth": "^8.4|^9.0|^10.0",
        "doctrine/dbal": "^2.10|^3.1"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "friendsofphp/php-cs-fixer": "^2.16|^3.14",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "laravel/legacy-factories": "1.x-dev|^1.3",
        "nunomaduro/collision": "^5.0|^6.1",
        "nunomaduro/larastan": "^0.7.12|^2.4",
        "phpmd/phpmd": "^2.8",
        "phpunit/phpunit": "9.4.3|^9.5.10",
        "squizlabs/php_codesniffer": "*",
        "orchestra/testbench": "^5.0|^6.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "IFRS\\": "src"
        },
        "classmap": [
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "IFRS\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IFRS\\IFRSServiceProvider"
            ]
        }
    }
}

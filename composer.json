{
	"name" : "ekmungai/eloquent-ifrs",
	"description" : "Eloquent Double Entry Accounting with focus on IFRS Compliant Reporting",
	"keywords" : [
		"accounting",
		"eloquent",
		"laravel",
		"lumen",
		"reporting"
	],
	"type" : "library",
	"license" : "MIT",
	"support" : {
		"issues" : "https://github.com/ekmungai/eloquent-ifrs/issues",
		"source" : "https://github.com/ekmungai/eloquent-ifrs"
	},
	"authors" : [{
			"name" : "Edward Mungai",
			"email" : "emungai@karanjamungai.com"
		}
	],
	"minimum-stability" : "dev",
	"require" : {
		"php" : "^7.3.0",
		"illuminate/database" : "^7.0|^8.40.0",
		"illuminate/auth" : "^7.0|^8.40.0",
		"calebporzio/parental" : "^0.11.0",
		"doctrine/dbal": "^2.10.1"
	},
	"require-dev" : {
		"facade/ignition" : "^2.3.6",
		"friendsofphp/php-cs-fixer" : "^2.16",
		"fzaninotto/faker" : "^1.4",
		"mockery/mockery" : "^1.0",
		"nunomaduro/collision" : "^5.0",
		"nunomaduro/larastan" : "^0.5.0",
		"phpmd/phpmd" : "^2.8",
		"phpunit/phpunit" : "9.5.6",
		"squizlabs/php_codesniffer" : "*",
		"orchestra/testbench" : "^5.0|^6.0"
	},
	"autoload" : {
		"psr-4" : {
			"IFRS\\" : "src"
		},
		"classmap" : [
			"database/factories"
		]
	},
	"autoload-dev" : {
		"psr-4" : {
			"IFRS\\Tests\\" : "tests"
		}
	},
	"extra" : {
		"laravel" : {
			"providers" : [
				"IFRS\\IFRSServiceProvider"
			]
		}
	}
}
